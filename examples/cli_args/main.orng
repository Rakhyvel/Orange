import std::fmt

// The `Args` context allows functions to access the command-line arguments
fn main() -> ()!() with (core::IO, core::Args) {
    try @println("given {core::Args.args.length} args:")

    for i in core::Range(0, core::Args.args.length) {
        try @println("arg[{i}]: {core::Args.args[i]}")
    }

    .ok
}