// fizzbuzz
fn main()-> String
    fizzbuzz(30).string

let const FizzBuzzResult: Type
    = string: String
    | integer: Int

fn fizzbuzz(n: Int) -> FizzBuzzResult
    case
    | n % 15 == 0 => .string <- "fizzbuzz"
    | n % 5 == 0  => .string <- "buzz"
    | n % 3 == 0  => .string <- "fizz"
    | else        => .integer <- n