// 395
fn main() -> Int {
    parse_int("395")
}

fn parse_int(buffer: String) -> Int {
    let mut retval = 0
    let mut i = 0
    
    if buffer[0] == '-' {
        i += 1
    } else if buffer[0] == '+' {
        i += 1
    }

    while i < buffer.length; i += 1 {
        let c: Byte = buffer[i]
        if c >= '0' and c <= '9' {
            retval = retval * 10 + (c as Int - '0')
        } else {
            break
        }
    }

    retval
}