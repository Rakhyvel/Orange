// 365
struct Thingy[T] {
    x: Int
    y: Int
}

context My_Context {
    x: Int
}

trait Sized {
    const size: Int
}

type My_Type = [45]Int
impl Sized for My_Type { const size: Int = 360 }

impl[T: Sized] for Thingy[T] {
    fn init() -> Self with My_Context {
        Self(0, 0)
    }

    fn get_val(&mut self) -> Int {
        self.>calc_size()
        self.x
    }

    fn calc_size(&mut self) {
        self.x = T::size
    }
}

fn main() -> Int {
    with My_Context(0) {
        let mut my_thingy = Thingy[My_Type]::init()
        my_thingy.>get_val() + 5
    }
}