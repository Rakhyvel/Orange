// 366

context My_Context {
    x: Int
}

trait Contexual {
    virtual fn virt_contexual(self) -> Int with My_Context

    fn static_contexual(self) -> Int with My_Context
}

impl Contexual for Int {
    virtual fn virt_contexual(self) -> Int with My_Context {
        My_Context.x + self
    }

    fn static_contexual(self) -> Int with My_Context {
        My_Context.x + self
    }
}

fn main() -> Int {
    with My_Context(182) {
        let x: Int = 1
        x.>virt_contexual() + x.>static_contexual()
    }
}