// 250
let const My_Type: Type = (
    x: Int = 450,
    y: Int = 50,
)

fn main() -> Int {
    add_fields(2)
}

// A way to provide a default value for a type
// This is _really_ useful for generic functions (soon!)
fn add_fields(scale: Int, mt: My_Type = default My_Type) -> Int {
    (mt.x + mt.y) / scale
}