// 193
fn main() -> Int {
    if close_enough(2.0 ** 4.0, 16.0, 0.01) {
        193
    } else {
        4
    }
}

fn close_enough(a: Float, b: Float, e: Float) -> Bool {
    let diff: Float = if a == b {
        return true
    } else if a > b {
        a - b
    } else {
        b - a
    }
    diff <= e
}