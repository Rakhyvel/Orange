// 268
let const constant_32: Int32 = 45
let const constant_64: Int = 268

let const system: (bits_64 | bits_32) = .bits_64

fn main() -> Int {
    if comptime (system == .bits_32) {
        constant_32 // must be syntactically correct, but not type checked if reduced!
    } else {
        constant_64
    }
}