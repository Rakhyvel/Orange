// 381
trait Value { virtual fn get(&self) -> Int }
impl Value for Int { virtual fn get(&self) -> Int { self^ } }

fn use_dyn[T: Value](x: &T) -> Int {
    let dyn_val: &dyn Value = &x // should auto-deref T to match dyn Value
    dyn_val.>get()
}

fn main() -> Int {
    let val = 381
    use_dyn[Int](&val)
}